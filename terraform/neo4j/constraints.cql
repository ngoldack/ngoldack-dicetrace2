CREATE CONSTRAINT IF NOT EXISTS C_USER__USERNAME_UNIQUE FOR (u:USER) REQUIRE u.username IS UNIQUE
CREATE CONSTRAINT IF NOT EXISTS C_USER__EMAIL_UNIQUE FOR (u:USER) REQUIRE u.email IS UNIQUE;

CREATE CONSTRAINT C__UUID_UNIQUE IF NOT EXISTS FOR (a:) REQUIRE a.uuid IS UNIQUE

CREATE CONSTRAINT ON (person:Person) ASSERT person.uuid IS UNIQUE